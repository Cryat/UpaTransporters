    </message>

    <message name="BadLocationFault">
        <part name="fault" element="tns:BadLocationFault" />
    </message>
    <message name="BadPriceFault">
        <part name="fault" element="tns:BadPriceFault" />
    </message>
    <message name="BadJobFault">
